#------------------------------------------------------------------
# opMODE;
#define BYPASS			2
#define NORMAL			3
parameter {
	name = opMODE
	value = 3
}
#------------------------------------------------------------------
# fo_freqHz: Carrier frequency
parameter {
	name = f0_freqHz
	value = 5000.0
}
#------------------------------------------------------------------
# gain: Output gain
parameter {
	name = gain
	value = 1.0
}
#------------------------------------------------------------------
# Ninterpol: Interpolation Number. Add N-1 zeros. 60
parameter {
	name = Ninterpol
	value = 56
}
#------------------------------------------------------------------
# Ndecimate: Decimation Number. Eliminate samples
parameter {
	name = Ndecimate
	value = 15
#------------------------------------------------------------------
# samplingfreqHz: A value in Hz. Use 1.0 when refers to samples
parameter {
	name = samplingfreqHz
	value = 48000.0
}
#------------------------------------------------------------------
# Name of selected filter coeficients file
parameter {
	name = filtercoefffile
	value = paramscfg/FILTRO.coeff
}
#------------------------------------------------------------------





